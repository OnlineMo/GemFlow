{
  "name": "gemflow",
  "version": "1.0.0",
  "description": "GemFlow：基于 LangGraph 与 Gemini 的服务端深度思考与工具编排库",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "example:basic": "tsx examples/basic.ts",
    "example:structured": "tsx examples/structured.ts",
    "example:streaming": "tsx examples/streaming.ts",
    "example:tools": "tsx examples/tools.ts"
  },
  "keywords": [
    "gemflow",
    "langgraph",
    "gemini",
    "ai",
    "llm",
    "reasoning",
    "tools",
    "orchestration"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@langchain/core": "^0.3.0",
    "@langchain/google-genai": "^0.1.0",
    "@langchain/langgraph": "^0.2.0",
    "langchain": "^0.3.0",
    "lru-cache": "^10.2.0",
    "p-limit": "^5.0.0",
    "p-retry": "^6.2.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.0",
    "tsup": "^8.1.0",
    "tsx": "^4.11.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  }
}